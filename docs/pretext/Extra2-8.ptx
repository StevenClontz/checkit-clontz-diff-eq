    <answer>
        <ol>
        <!--  -->
            <li>
                <!--  -->
                <p>
                    <m>{y} = 4 \, t^{2} - \frac{2}{t^{3}}</m>
                </p>
                <!--  -->
                <!--  -->
            </li>
        <!--  -->
            <li>
                <!--  -->
                <p>
                    <m>-4 \, t^{2} y^{2} - 2 \, t y - 5 \, y^{2} = \left(-7\right)</m>
                </p>
                <!--  -->
                <!--  -->
            </li>
        <!--  -->
            <li>
                <!--  -->
                <p>
                    <m>{y} = -4 \, e^{\left(3 \, t\right)}</m>
                </p>
                <!--  -->
                <!--  -->
            </li>
        <!--  -->
            <li>
                <!--  -->
